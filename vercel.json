{
    "version": 2,
    "routes": [
      {
        "src": "/",
        "dest": "/api/index.js"
      },
      {
        "src": "/user/.*",
        "dest": "api/user.js"
      },
      {
        "src": "/404",
        "dest": "api/404.js"

      },
      {
        "src": "/new/.*",
        "dest": "api/new.js"
      },
      {
        "src": "/all",
        "dest": "api/all.js"
      },
      {
        "src": "/save",
        "dest": "api/save.js"
      },
      {
        "src": "/latest",
        "dest": "api/latestPost.js"
      },
      {
        "src": "/.*",
        "dest": "api/redirect.js"
      }

     
    ]
  }